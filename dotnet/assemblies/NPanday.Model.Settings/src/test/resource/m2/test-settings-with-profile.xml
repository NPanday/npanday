          <releases />
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>npanday.repo.0</id>
          <url>http://repo1.maven.org/maven2</url>
        </repository>
      </repositories>
      <id>npanday.test.id</id>
    </profile>
  </profiles>
